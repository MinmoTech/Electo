{
  "main": "node_modules/expo/AppEntry.js",
  "name": "electo",
  "author": {
    "name": "Julius Dehner",
    "email": "julius.dehner@gmail.com"
  },
  "version": "0.0.1",
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ],
    "appId": "de.electo",
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "electron-start": "yarn expo-electron start",
    "electron-test-build": "yarn electron-webpack && yarn electron-builder -mwl -c.compression=store -c.mac.identity=null",
    "electron-prod-build": "yarn electron-webpack && yarn electron-builder -mwl -c.mac.identity=null"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "expo": "~38.0.8",
    "expo-status-bar": "^1.0.2",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-web": "~0.11.7",
    "react-player": "^2.6.0",
    "shaka-player-react": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/electron-adapter": "^0.0.8",
    "@expo/webpack-config": "^0.12.21",
    "babel-preset-expo": "~8.1.0",
    "electron": ">=18.3.7",
    "electron-builder": "^22.8.0"
  },
  "private": true
}
